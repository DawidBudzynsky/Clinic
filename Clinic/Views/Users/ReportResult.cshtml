@model List<DoctorReportViewModel>

<h2>Doctor Report</h2>

<table class="table">
    <thead>
        <tr>
            <th hidden>Doctor Id</th>
            <th>Doctor Username</th>
            <th>Scheduled Time</th>
            <th>Visits Count</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td hidden>@item.doctorId</td>
                <td>@item.doctorUsername</td>
                <td>@item.scheduledTime</td>
                <td>@item.visitCount</td>
            </tr>
        }
    </tbody>
</table>
