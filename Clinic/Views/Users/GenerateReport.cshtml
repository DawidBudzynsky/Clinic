
<form asp-action="GenerateReport" asp-controller="Users" method="post">
    <label for="startDate">Start Date:</label>
    <input type="datetime-local" id="startDate" name="t1" required>

    <label for="endDate">End Date:</label>
    <input type="datetime-local" id="endDate" name="t2" required>

    <button type="submit">Generate Report</button>
</form>